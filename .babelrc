{
	"presets": ["@babel/preset-env", "@babel/preset-react"],
	"env": {
		"development": {
			"presets": [
				[
					"@babel/preset-react", {
					"development": true
				}
				]
			]
		},
		"production": {
			"plugins": [
				[
					"transform-remove-console", {
					"exclude": ["error", "warn"]
				}
				]
			]
		}
	}
}
